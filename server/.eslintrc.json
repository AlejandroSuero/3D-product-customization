{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error"
        ],
        "indent": [
            "error",
            2
        ],
        "no-unused-vars": [
            "warn"
        ],
        "import/extensions": [
            "off"
        ]
    }
}